<div class="container border rounded p-5" id="sign-in">
  <h2 class="text-center">LOGIN</h1>
  <%= simple_form_for :session, url: session_path do |f| %>
    <%= f.input :email %>
    <%= f.input :password %>

    <%= f.submit "Sign In", class: "btn btn-primary btn-lg btn-block" %>
  <% end %>
</div>